<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
           xmlns:avc="http://xml.avc.mx/school/logicclock"
           elementFormDefault="qualified" attributeFormDefault="unqualified"
           targetNamespace="http://xml.avc.mx/school/logicclock">
    <xs:element name="packet" type="avc:packet"/>
    <xs:complexType name="packet">
        <xs:sequence>
            <xs:element name="payload" type="xs:normalizedString"/>
            <xs:element name="timestamp" type="xs:long"/>
        </xs:sequence>
    </xs:complexType>
</xs:schema>


